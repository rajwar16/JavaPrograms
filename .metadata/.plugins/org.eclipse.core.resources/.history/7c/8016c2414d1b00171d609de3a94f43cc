import java.util.Scanner;

public class TofindYourNo 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the value : ");
		int N=sc.nextInt();
		System.out.println("Assume No in between 0 to "+(N-1));
		int high=N-1;
		int low=0;
		while(low<=high)
		{
			int mid=low+high/2;
			System.out.println("assume ur no is "+mid+". if yes press ok. ");
			System.out.println("if ur no is smaller than "+mid+". type true.");
			System.out.println("if ur no is greater than "+mid+". type false.");
		}


	}
}
